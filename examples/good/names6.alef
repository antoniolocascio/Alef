(* Examples of the importance of effect variable names. *)

g : unit -> unit<Op1 | mu3>
g = fun u -> Op1();;

f : unit -> unit<mu55>
f = fun u -> val u;;

ign : (unit -> unit<mu1>) -> unit<mu2>
